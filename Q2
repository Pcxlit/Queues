#include <iostream>
using namespace std;
class circularaqueue
{
    int front;
    int rear;
    int *arr;
    int size;

public:
    circularaqueue()
    {
        int size = 1000;
        arr = new int[size];
        front = -1;
        rear = -1;
    }
    int enqueueu(int n)
    {
        if ((front == 0 && rear == (size-1)) || (rear == (front - 1) % (size-1)))
        {
            cout << "queueu is full " << endl;
        }
        else if (front == -1)
        {
            front = rear = 0;
            arr[rear] = n;
            rear++;
        }
        else if (rear == size - 1 && front != 0)
        {
            rear = 0;
            arr[rear] = n;
            rear++;
        }
        else
        {
            arr[rear] = n;
            rear++;
        }
    }
    int dequeue(){
        if (front==-1)
        {
            cout<<"queueu is empty "<<endl;
        }
        int ans=arr[front];
        arr[front]=-1;
        if (front==rear)
        {
            front=rear=-1;
        }
        else if (front==size-1)
        {
            front=0;
        }
        else
        {
            front++;
        }
        
        
        
        
    }

};
int main()
{
    return 0;
}
